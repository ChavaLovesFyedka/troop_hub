<%= form_for(@scout) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br/>
  <br/>
  <%= f.label :grade %>
  <%= f.text_field :grade %><br/>
    <br/>

<%= f.label :birthday %>:
<%= date_select :birthday, "Birthday" %>

<br/>
  <%= f.label :email %>
  <%= f.email_field :email %><br/>
    <br/>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %><br/>
    <br/>
  <%= f.label :profile_photo %>
  <%= f.file_field :profile_photo %><br/>
    <br/>
  <br/>

    <br/>
  <%= f.submit %>
<% end %>